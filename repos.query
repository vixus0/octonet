query($org: String!, $teamSlug: String!, $repoCursor: String){
  rateLimit{
    cost
    limit
    nodeCount
    remaining
    resetAt
  }
  
  organization(login: $org) {
    team(slug: $teamSlug) {
      repositories(first: 100, after: $repoCursor) {
        pageInfo {
          endCursor
          hasNextPage
        }
        edges {
          node {
            name
            parent {name}
          }
          permission
        }
      }
    }
  }
}
